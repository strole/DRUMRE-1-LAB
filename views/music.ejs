<!doctype html>
<html>
<head>
    <title>Music</title>
    <link rel="stylesheet" href="//netdna.bootstrapcdn.com/bootstrap/3.0.2/css/bootstrap.min.css">
    <link rel="stylesheet" href="//netdna.bootstrapcdn.com/font-awesome/4.0.3/css/font-awesome.min.css">
    <style>
        body        { padding-top:80px; word-wrap:break-word; }
    </style>
</head>
<body>
    <div class="page-header text-center">
        <h1><span class="fa"></span> Music Page</h1>
        <a href="/logout" class="btn btn-default btn-sm">Logout</a>
        <a href="/profile" class="btn btn-default btn-sm">Profile</a>
        <a href="/movies" class="btn btn-default btn-sm">Movies</a>
        <a href="/music" class="btn btn-default btn-sm">Music</a>
    </div>


<div class="container">

    <h2>Pjesme</h2>
    <% for(var i = 0; i < songList.length; i++) { %>
        <p class="well">
            <span>
            <img src="<%=songList[i].image%>" width="200" height="200" alt="">
        </span>
        <span>
            <strong style="font-size: xx-large;"> <%= songList[i].name %> </strong> <br>
            <strong>Url</strong>:</strong> <%= songList[i].url %> </strong> <br>
            <strong>Artist</strong>:</strong> <%= songList[i].artist %> </strong> <br>
        </span>
        </p>
        <br>
  <%  } %>

    
    
</div>
</body>
</html>